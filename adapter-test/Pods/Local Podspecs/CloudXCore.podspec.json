{
  "name": "CloudXCore",
  "version": "1.1.34",
  "summary": "CloudX Core Framework",
  "description": "Core framework for CloudX functionality",
  "homepage": "https://github.com/cloudx-xenoss/CloudXCoreiOS",
  "license": {
    "type": "Business Source License 1.1",
    "file": "LICENSE"
  },
  "authors": {
    "CloudX": "support@cloudx.io"
  },
  "source": {
    "http": "https://github.com/cloudx-xenoss/CloudXCoreiOS/releases/download/v1.1.4/CloudXCore.xcframework.zip"
  },
  "platforms": {
    "ios": "14.0"
  },
  "source_files": "Sources/CloudXCore/**/*.{h,m}",
  "resource_bundles": {
    "CloudXCore": [
      "Sources/CloudXCore/AdReporting/CoreData/CloudXDataModel.xcdatamodeld"
    ]
  },
  "frameworks": [
    "SafariServices",
    "CoreData"
  ],
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "$(PODS_ROOT)/CloudXCore/Sources/CloudXCore",
    "DEFINES_MODULE": "YES",
    "CLANG_ENABLE_MODULES": "YES",
    "ENABLE_USER_SCRIPT_SANDBOXING": "NO"
  },
  "user_target_xcconfig": {
    "ENABLE_USER_SCRIPT_SANDBOXING": "NO"
  }
}
